    "build": "npm run compress-modules && next build",
    "start": "next start -H 0.0.0.0",
    "lint": "next lint",
    "test:sync": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"CommonJS\"}' node --require ts-node/register --require tsconfig-paths/register --test tests/manifest.test.ts",
